{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "C:\\tools\\msys64\\ucrt64\\bin\\g++.exe",
			"args": [
				"-g", /**/"${file}",/*/
				"${workspaceFolder}\\",/**/
				"-o", "${fileDirname}\\${fileBasenameNoExtension}.exe",
				"-IC:\\tools\\msys64\\ucrt64\\include",
				"-I${workspaceFolder}\\Headers",
				"-llibglfw3",
				"-luuid",
				"-lmsimg32",
				"-lgdi32",
				"-limm32",
				"-lole32",
				"-loleaut32",
				"-lwinmm",
				"-lgdiplus",
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
